<!DOCTYPE html>
<html>
  <% include partials/header.ejs %>
  <body>
    <div class="d-flex" id="wrapper">
      <% include partials/sidebar.ejs %>
      <!-- Page Content -->
      <div id="page-content-wrapper">
        <% include partials/nav.ejs %>
        <div class="container-fluid">

          <h1><%= title %></h1>

          <form action="/articles/add" method="post">
            <div class="form-group">
              <label for="exampleFormControlInput1">Center Name</label>
              <input type="text" class="form-control" id="exampleFormControlInput1" name="title">
            </div>
            <div class="form-group">
              <label for="exampleFormControlInput1">Township</label>
              <select type="text" class="form-control" name="category">
                <option value="bogalay">Bogalay</option>
                <option value="danubyu">Danubyu</option>
                <option value="dedaye">Dedaye</option>
                <option value="einme">Einme</option>
                <option value="hinthada">Hinthada</option>
                <option value="kyaunggone">Kyaunggone</option>
                <option value="kyonepyaw">Kyonepyaw</option>
                <option value="maubin">Ma-ubin</option>
                <option value="myanaung">Myanaung</option>
                <option value="myaungmya">Myaungmya</option>
                <option value="pantanaw">Pantanaw</option>
                <option value="pathein">Pathein</option>
                <option value="wakema">Wakema</option>
                <option value="zalun">Zalun</option>
              </select>
            </div>
            <div class="form-group">
              <label for="exampleFormControlInput1">Address</label>
              <input type="text" class="form-control" id="exampleFormControlInput2" name="address">
            </div>
            <div class="form-group">
              <label for="exampleFormControlInput1">phone</label>
              <input type="number" class="form-control" id="exampleFormControlInput3" name="phone">
            </div>
            <div class="form-group">
              <label for="exampleFormControlInput1">Open time</label>
              <input type="time" class="form-control" id="exampleFormControlInput4" name="opttime">
            </div>
            <div class="form-group">
              <label for="exampleFormControlInput1">Close time</label>
              <input type="time" class="form-control" id="exampleFormControlInput5" name="closetime">
            </div>
            <div class="form-group">
              <label for="exampleFormControlInput1">Member feed</label>
              <input type="nember" class="form-control" id="exampleFormControlInput6" name="memberfeed">
            </div>
            <div class="form-group">
              <label for="photo">Photo:</label>
              <input type="file" name="photo" id="photo" accept="image/*"><p></p>
            </div>
            <div class="form-group">
              <label for="exampleFormControlInput1">Select Class</label>
              <select class="mdb-select md-form" multiple name="classes">
                <option value="" disabled selected>Choose your Class</option>
                <option value="1" name="classes">Yoga</option>
                <option value="2" name="classes">Kickboxing</option>
                <option value="3" name="classes">Personal Training</option>
                <option value="4" name="classes">Zumba</option>
                <option value="5" name="classes">Kilocycle</option>
                <option value="6" name="classes">Barre-less-barre</option>
                <option value="7" name="classes">Cardio funk Dance Party</option>
              </select>
            </div>
            <div class="form-group">
              <label for="exampleFormControlTextarea1">Other Infomation</label>
              <textarea class="form-control" id="exampleFormControlTextarea1" rows="3" name="otinfo">
              </textarea>
            </div>
            <button type="submit">Submit</button>
          </form>

        </div>
      </div>
    </div>
    <% include partials/footer.ejs %>
  </body>
  <% include partials/scripts.ejs %>
</html>
